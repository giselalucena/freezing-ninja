package projeto.jfc;

import java.awt.event.ActionEvent;

import javax.swing.AbstractAction;
import javax.swing.JOptionPane;


public class CadastroFuncionarioAction extends AbstractAction {
	
	private static final long serialVersionUID = 1L;
	private CadastroFuncionario frame;
	
	public CadastroFuncionarioAction (CadastroFuncionario frame){
		super("Cadastrar Funcionário");
		this.frame = frame;
	}
	
	
	
	
	
	@Override
	public void actionPerformed(ActionEvent arg0) {
		System.out.println("Cadastro Funcionário");
		JOptionPane.showInputDialog(frame);               
	}

}
