package projeto.jfc;

import java.awt.event.ActionEvent;

import javax.swing.AbstractAction;


public class CadastroFuncionarioAction extends AbstractAction {
	
	private static final long serialVersionUID = 1L;
	private CadastroFuncionarioPanel panel;
	
	public CadastroFuncionarioAction (CadastroFuncionarioPanel panel){
		super("Cadastrar Funcionário");
		this.panel = panel;
	}
	
	
	
	
	
	@Override
	public void actionPerformed(ActionEvent arg0) {
		System.out.println("Cadastro Funcionário");
		
	}

}
