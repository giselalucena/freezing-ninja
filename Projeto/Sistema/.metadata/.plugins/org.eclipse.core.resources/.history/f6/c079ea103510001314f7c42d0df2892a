package projeto.pojo;

public class Funcionario {
		private String nome;		
		private int rg;
		private int cpf;
		//dataNasc date;
		private String nomeMae;
		private String endereco;
		private int cep;
		private int telefone;
		private String email;
		private int conselho;
		private int matricula;
		private int secretaria;
		private int cargo;
		private int funcao;
		private int cargaHorariaSemanal; 

public Funcionario(String nome,int rg,int cpf, String nomeMae, String endereco, int cep, int telefone, String email, int conselho,
		int matricula, int secretaria, int cargo, int funcao, int cargaHorariaSemanal){
	super();
	this.nome = nome;
	this.rg = rg;
	this.cpf = cpf;
	this.nomeMae = nomeMae;
	this.endereco = endereco;
	this.cep = cep;
	this.telefone = telefone;
	this.email = email;
	this.conselho = conselho;
	this.matricula = matricula;
	this.secretaria = secretaria;
	this.cargo = cargo;
	this.funcao = funcao;
	this.cargaHorariaSemanal = cargaHorariaSemanal;
	
}
public Funcionario(String nome,int rg,int cpf, String nomeMae, String endereco, int cep, int telefone, String email, 
		int matricula, int secretaria, int cargo, int funcao, int cargaHorariaSemanal){
	super();
	this.nome = nome;
	this.rg = rg;
	this.cpf = cpf;
	this.nomeMae = nomeMae;
	this.endereco = endereco;
	this.cep = cep;
	this.telefone = telefone;
	this.email = email;
	this.matricula = matricula;
	this.secretaria = secretaria;
	this.cargo = cargo;
	this.funcao = funcao;
	this.cargaHorariaSemanal = cargaHorariaSemanal;
	
}
		
		
		public String getNome() {
			return nome;
		}
		public int getRg() {
			return rg;
		}
		public int getCpf() {
			return cpf;
		}
		public String getNomeMae() {
			return nomeMae;
		}
		public String getEndereco() {
			return endereco;
		}
		public int getCep() {
			return cep;
		}
		public int getTelefone() {
			return telefone;
		}
		public String getEmail() {
			return email;
		}
		public int getConselho() {
			return conselho;
		}
		public int getMatricula() {
			return matricula;
		}
		public int getSecretaria() {
			return secretaria;
		}
		public int getCargo() {
			return cargo;
		}
		public int getFuncao() {
			return funcao;
		}
		public int getCargaHorariaSemanal() {
			return cargaHorariaSemanal;
		}
}
